{
    "name": "@kbase/ui-lib",
    "version": "0.2.0-alpha.3",
    "description": "A typescript library to support kbase-ui and plugins",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "/lib/**/*"
    ],
    "scripts": {
        "test": "jest",
        "copy-assets": "copyfiles -u 1  \"./src/**/*.css\" \"./lib/\"",
        "clean": "rm -rf ./lib/*",
        "build": "bash scripts/build.bash",
        "prepare": "bash scripts/prepare.bash",
        "prepublish": "jest",
        "start": "tsc --watch",
        "generate-api-docs": "typedoc --out api-docs src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kbaseIncubator/kbase-ui-lib.git"
    },
    "keywords": [
        "kbase",
        "ui",
        "lib",
        "react",
        "typescript"
    ],
    "author": "KBase Developers",
    "license": "SEE LICENSE IN LICENSE.md",
    "bugs": {
        "url": "https://github.com/kbaseIncubator/kbase-ui-lib/issues"
    },
    "homepage": "https://github.com/kbaseIncubator/kbase-ui-lib#readme",
    "dependencies": {
        "@types/bluebird": "*",
        "@types/uuid": "*",
        "axios": "^0.21.1",
        "bluebird": "^3.5.5",
        "es-cookie": "^1.2.0",
        "uuid": "^3.3.3"
    },
    "devDependencies": {
        "@types/enzyme": "3.10.3",
        "@types/enzyme-adapter-react-16": "1.0.5",
        "@types/jest": "*",
        "copyfiles": "2.1.1",
        "eslint": "6.3.0",
        "eslint-plugin-react": "7.14.3",
        "jest": "24.9.0",
        "jest-transform-css": "2.0.0",
        "ts-jest": "24.0.2",
        "typedoc": "0.15.0",
        "typescript": "3.6.2"
    },
    "publishConfig": {
        "access": "public"
    }
}